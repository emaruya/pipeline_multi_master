pipeline {
    agent any

    parameters {
        string(name: 'AMI_ID', defaultValue: '', description: 'id da ami')
    }

    stages {
        stage('Build cluster') {
            steps {
                sh "echo \"--- ${VERSION} ----\""
                echo "Building.. ${VERSION}"
            }
        }
        stage('Docker image') {
            steps {
                sh 'ls -la'
                echo 'Building..'
            }
        }
        stage('Docker image Push') {
            steps {
                sh 'ls -la'
                echo 'Building..'
            }
        }
        stage('TDD') {
            steps {
                sh 'ls -la'
                echo 'Building..'
            }
        }
        
        stage('K8s-DEV') {
            steps {
                sh 'ls -la'
                echo 'Building..'
            }
        }
        stage('K8s-QA') {
            steps {
                sh 'ls -la'
                echo 'Building..'
            }
        }
        stage('K8s-PROD') {
            steps {
                sh 'ls -la'
                echo 'Building..'
            }
        }
    }
}
